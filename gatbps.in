    case "${?}" in
      0)
        :
      ;;
      *)
        cat >&2 <<EOF
gatbps: unrecognized options: ${1}
gatbps: try "gatbps --help" for more help
EOF
      ;;
    esac

    case "${option}" in
      -)
        option='-
'
      ;;
    esac

    cat >&2 <<EOF
gatbps: unrecognized option: ${option}
gatbps: try "gatbps --help" for more help
EOF
    exit 1

  ;;

esac

