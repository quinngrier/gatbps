<https://creativecommons.org/publicdomain/zero/1.0/>.
EOF
    case "${?}" in
      0)
        :
      ;;
      *)
        cat >&2 <<'EOF'
gatbps: cat failed to write to standard output
EOF
        exit 1
      ;;
    esac

    exit 0

  ;;

  --version=*)

    cat >&2 <<'EOF'
gatbps: --version does not accept an argument
gatbps: try "gatbps --help" for more help
EOF
    exit 1

  ;;

  --)

    shift

  ;;

  --=*)

    cat >&2 <<'EOF'
gatbps: -- does not accept an argument
gatbps: try "gatbps --help" for more help
EOF
    exit 1

  ;;

  --*=*)

    option=`expr "X${1}" : 'X\([^=]*\)' 2>/dev/null`
    case "${?}" in
      0)
        :
      ;;
      *)
        cat >&2 <<EOF
gatbps: unrecognized option: ${1}
gatbps: try "gatbps --help" for more help
EOF
        exit 1
      ;;
    esac

    case "${1}" in
      "${option}"=*)
        :
      ;;
      *)
        cat >&2 <<EOF
gatbps: unrecognized option: ${1}
gatbps: try "gatbps --help" for more help
EOF
        exit 1
      ;;
    esac

    cat >&2 <<EOF
gatbps: unrecognized option: ${option}
gatbps: try "gatbps --help" for more help
EOF
    exit 1

